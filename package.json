{
  "name": "homepage",
  "version": "1.0.0",
  "description": "ナルミンチョの創作記録 https://narumincho.com ",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@narumincho/html": "0.1.3",
    "@types/fs-extra": "9.0.4",
    "@types/node": "14.14.12",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "fs-extra": "9.0.1",
    "prettier": "2.2.1",
    "typescript": "4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narumincho/HomePage.git"
  },
  "author": "narumincho <narumincho.starfy@gmail.com> (https://narumincho.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/narumincho/HomePage/issues"
  },
  "homepage": "https://github.com/narumincho/HomePage#readme",
  "eslintConfig": {
    "extends": [
      "eslint:all",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module",
      "project": [
        "./source/tsconfig.json",
        "./source/script/tsconfig.json"
      ]
    },
    "rules": {
      "no-constant-condition": [
        "error",
        {
          "checkLoops": false
        }
      ],
      "no-else-return": [
        "error",
        {
          "allowElseIf": false
        }
      ],
      "one-var": [
        "error",
        "never"
      ],
      "sort-keys": [
        "off"
      ],
      "no-magic-numbers": [
        "off"
      ],
      "max-lines-per-function": [
        "off"
      ],
      "no-undefined": [
        "off"
      ],
      "default-case": [
        "off"
      ],
      "no-ternary": [
        "off"
      ],
      "id-length": [
        "off"
      ],
      "consistent-return": [
        "off"
      ],
      "new-cap": [
        "off"
      ],
      "no-use-before-define": [
        "off"
      ],
      "max-lines": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "max-params": [
        "off"
      ],
      "prefer-template": [
        "off"
      ],
      "spaced-comment": [
        "off"
      ],
      "no-bitwise": [
        "off"
      ],
      "capitalized-comments": [
        "off"
      ],
      "array-callback-return": [
        "off"
      ],
      "max-statements": [
        "off"
      ],
      "prefer-destructuring": [
        "off"
      ],
      "yoda": [
        "off"
      ],
      "@typescript-eslint/restrict-plus-operands": [
        "error",
        {
          "checkCompoundAssignments": true
        }
      ],
      "@typescript-eslint/array-type": [
        "error",
        {
          "default": "generic"
        }
      ],
      "@typescript-eslint/consistent-type-definitions": [
        "error",
        "type"
      ],
      "@typescript-eslint/no-dynamic-delete": [
        "error"
      ],
      "@typescript-eslint/no-require-imports": [
        "error"
      ],
      "@typescript-eslint/no-unnecessary-qualifier": [
        "error"
      ],
      "@typescript-eslint/no-unused-expressions": [
        "error"
      ],
      "@typescript-eslint/prefer-for-of": [
        "error"
      ],
      "@typescript-eslint/require-array-sort-compare": [
        "error"
      ],
      "@typescript-eslint/no-use-before-define": [
        "error",
        {
          "variables": false
        }
      ],
      "@typescript-eslint/no-empty-function": [
        "off"
      ]
    }
  }
}
